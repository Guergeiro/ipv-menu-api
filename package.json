  "bugs": {
    "url": "https://github.com/Guergeiro/ementas-api/issues"
  },
  "homepage": "https://github.com/Guergeiro/ementas-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "form-data": "^3.0.0",
    "helmet": "^3.21.2",
    "node-fetch": "^3.1.1",
    "xlsx": "^0.15.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "0.0.45",
    "@types/node": "^13.7.1",
    "@types/node-fetch": "^2.5.4",
    "@types/xlsx": "0.0.36",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
