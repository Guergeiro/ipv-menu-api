{
  "name": "ementas-api",
  "version": "1.0.0",
  "description": "A simple API that wraps around the cafeteria's menu from ESTGV",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules/",
    "compile": "tsc -p .",
    "prettify": "prettier --write **/*.ts",
    "build": "npm run prettify && npm run compile",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development nodemon src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guergeiro/ementas-api.git"
  },
  "author": "Breno Salles",
